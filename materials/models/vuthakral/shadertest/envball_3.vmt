"VertexLitGeneric"
{
	"$baseTexture" "Models/ShaderTest/envball_3"
	"$envmap" "env_cubemap"
	"$bumpmap" "models/shadertest/shieldnoise0_normal"
	"$normalmapalphaenvmapmask" 1
//	"$envmaptint" "[.5 .5 .5]"
	"$envmapsaturation" "[1 1 1]"
	"$bumpframe" "0"

	$cubemappower	1
	$cubemaptint	"[ 3 3 3 ]"

	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$bumpmap" 
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 29.00
		}
		drc_ReflectionTint
		{
			resultVar	$envmaptint
		}
	}
}


